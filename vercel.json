{
    "version": 2,
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/",
        "methods": ["POST"],
        "dest": "app/main.py"
      }
    ]
  }
  