{
    "version": 2,
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/api",
        "dest": "app/main:app"
      },
      {
        "src": "/debug/(.*)",
        "dest": "app/main:app"
      },
      {
        "src": "/favicon.ico",
        "dest": "app/main:app"
      }
    ]
  }